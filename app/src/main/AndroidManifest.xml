            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name="com.snabbmaten.user.activities.WalletActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="3aef7a5b05836cf8166f23c95e43c52517bee917" />

        <activity
            android:name="com.snabbmaten.user.activities.ResetPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.snabbmaten.user.activities.SecretKeyActivity"
            android:screenOrientation="portrait" />

