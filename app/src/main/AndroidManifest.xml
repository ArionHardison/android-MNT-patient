        <activity
            android:name="com.snabbmaten.user.activities.ProductDetailActivity"
            android:parentActivityName="com.snabbmaten.user.activities.HotelViewActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>